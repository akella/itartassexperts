	i
		+sized-sprite($icons, btn-prev1)
.main-experts__btn-next
	right: 0
	i	
		+sized-sprite($icons, btn-next1)
.main-experts__list		
	position: absolute
	width: 2000em
	li
		position: relative
		float: left
		width: 200px
		margin-right: 14px
		box-shadow: rgba(0,0,0,0.45) -5px 0 10px
		text-align: center
.main-experts__item
	height: 300px	
	background: #FFF 	
	img
		margin-bottom: 10px
	a		
		display: block
		padding: 0 5px
		font-size: 21px
		color: #870c0c
		font-weight: bold
		line-height: 1.1  
		&:first-child
