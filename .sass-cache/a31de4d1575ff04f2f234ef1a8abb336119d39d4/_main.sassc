		position: absolute
		top: 0
		width: 200px
		height: 300px
		background: rgba(0,0,0,0.6) 
		z-index: 5 
		i
			position: absolute
			top: 118px			
			left: 50%
			margin-left: -8px
.main-experts__btn-prev
	left: 0
	i
		+sized-sprite($icons, btn-prev1)
.main-experts__btn-next
	right: 0
	i	
		+sized-sprite($icons, btn-next1)
.main-experts__list		
	position: absolute
	width: 2000em
	li
		position: relative
		float: left
		width: 200px
		margin-right: 14px
		box-shadow: rgba(0,0,0,0.45) -5px 0 10px
		text-align: center
.main-experts__item
	height: 300px	
	background: #FFF 	
	img
		margin-bottom: 10px
	a		
		display: block
		padding: 0 5px
		font-size: 21px
		color: #870c0c
		font-weight: bold
		line-height: 1.1  
		&:first-child
			padding: 0
	p
		height: 80px
		padding: 0 14px
		line-height: 80px
		font-size: 12px
		color: #282828 
	span
		display: inline-block
		line-height: 1.2
		vertical-align: middle
//main-events
.main-events
	padding-bottom: 125px
	.h2
		position: relative
		top: 12px
		font-size: 24px
		color: #fff
		text-align: center
		strong, span
			position: relative
		span
			z-index: 5 
		i
			position: absolute
			top: 15px
			left: -65px
			right: -65px
			height: 1px
			background: url(../img/events-bg.jpg)
.main-events__list	
	background: rgba(0,0,0,0.25) 
	border: 1px solid #696666
	border-width: 1px 0 1px 0
	ul
		width: 1270px
		margin: 0 auto
		text-align: center
		font-size: 0
	li
		position: relative
		display: inline-block	
		vertical-align: top	
		width: 240px
		&.disabled
			a
			.cr__ins
				+background(linear-gradient(#2c2c2c 0%, #6f6f6f 50%, #262626 51%, #262626 100%))
	a
		display: block
		height: 160px
		line-height: 160px	
		color: #FFF 	
		&:hover
			color: #4871c6
			.cr__ins
				i
					background: #03346c						
		span
			display: inline-block
			padding: 0 30px
			vertical-align: middle
			line-height: 1.4
			font-size: 13px			
.mein-events__date
	position: absolute
	bottom: -63px
	width: 100%
	text-align: center
	color: #FFF 
	font-size: 10px
	text-transform: uppercase
