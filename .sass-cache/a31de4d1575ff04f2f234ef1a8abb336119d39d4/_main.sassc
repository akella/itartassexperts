//main-events
.main-events
	padding-bottom: 125px
	.h2
		position: relative
		top: 12px
		font-size: 24px
		color: #fff
		text-align: center
		strong, span
			position: relative
		span
			z-index: 5 
		i
			position: absolute
			top: 15px
			left: -65px
			right: -65px
			height: 1px
			background: url(../img/events-bg.jpg)
.main-events__list	
	background: rgba(0,0,0,0.25) 
	border: 1px solid #696666
	border-width: 1px 0 1px 0
	ul
		width: 1270px
		margin: 0 auto
		text-align: center
		font-size: 0
	li
		position: relative
		display: inline-block	
		vertical-align: top	
		width: 240px
		&.disabled
			a
			.cr__ins
