//styles for inner pages
//aside-report
.aside-report
	position: relative	
.aside-report__detail
	padding: 25px 20px 0 20px		
	overflow: hidden
	img
		float: left
		margin: 20px 25px 30px 0
		border-radius: 2px
		box-shadow: rgba(0,0,0,0.75) 1px 1px 4px
	p
		margin-bottom: 30px
		overflow: hidden
		color: #FFF 
		span
			display: block
			margin-bottom: 5px
			font-size: 11px
		a
			font-size: 24px
			font-weight: bold
			line-height: 1.2
			color: #FFF 
.aside-report__more
	height: 50px
	margin: 0 20px
	border-top: 1px solid #366496
	line-height: 50px
	a
		font-size: 11px
		color: #fff
		text-transform: uppercase 
//aside-position
.aside-position__list
	padding: 0 20px 30px 20px
.aside-position__item
	padding-top: 18px
.aside-position__detail
	padding: 13px 12px 20px 12px
	box-shadow: rgba(0,0,0,0.25) 0 0 6px
	img
		float: left
		margin-right: 18px 
	p
		overflow: hidden
		a
			font-size: 16px
			color: #7c090d
			font-weight: bold
			line-height: 1.2
		span
			display: block
			margin-bottom: 7px
			a
				font-family: Georgia, Arial, Tahoma, sans-serif 
				font-weight: normal
				font-size: 13px
				color: #000
.aside-position__desc
	position: relative
	padding: 17px 12px 
	@extend %gradb
	border-radius: 3px
	font-size: 13px
	color: #FFF
	line-height: 1.1
	font-weight: bold 
	i
		position: absolute
		top: 0
		left: 35px
		width: 0px
		height: 0px
		border-style: solid
		border-width: 12px 7px 0 7px
		border-color: #ffffff transparent transparent transparent
//theme
.theme-list
	background: #eaeaea 
	.theme
		margin-bottom: 1px
	.theme__desc
		p
			margin-bottom: 14px
.theme
	position: relative
	padding: 15px 20px
	margin: 0 -1px
	border-radius: 3px
	@extend %gradb
	overflow: hidden
	z-index: 5 
.theme_red, .theme_content	
	.theme__comm
		a
			color: #FFF
		i
			+sized-sprite($icons, comm6)	
.theme_red
	@extend %gradr1
	.theme__comm
		border-color: #b85858
	.theme__desc
		.h1
			padding: 5px 0
			font-size: 36px
		p
			color: #FFF
.theme_content	
	.theme__desc
		padding-right: 20px
		overflow: hidden
		.h1
			padding: 10px 0 5px 0
	.theme__comm
		border-color: #5085b9
.theme__desc
	.h1
		padding: 5px 0 10px 0
		line-height: 1
	a
		color: #FFF 
	p, span
		font-size: 12px
		color: #a2b7d8
	p 
		line-height: 1.3
.theme__pic
	float: left
	margin: 7px 40px 7px 0
	img
		display: block
		border-radius: 2px
		box-shadow: rgba(0, 0, 0, 0.75) 1px 1px 4px
.theme__read
	position: absolute
	right: 20px
	bottom: 20px
	font-size: 14px
	color: #FFF 
//pagination
.pagination
	margin: -1px 0 50px 0
	padding: 28px 0 34px 0
	background: #fdfdfd
	border-radius: 0 0 3px 3px 
	text-align: center
	font-size: 0
	a
		display: inline-block 
		padding: 0 7px
		vertical-align: middle
		font-size: 12px
		color: #000  
		font-weight: bold
		&.active
			color: #676767 
	.pagination__prev, .pagination__next
		padding: 0
		&.disabled
			cursor: default
	.pagination__prev
		margin-right: 14px
		+sized-sprite($icons, btn-prev)
		&.disabled
			+sized-sprite($icons, btn-prev-dis)
	.pagination__next
		margin-left: 14px
		+sized-sprite($icons, btn-next)
		&.disabled
			+sized-sprite($icons, btn-next-dis)
.pagination_mod
	margin-bottom: 28px
	background: #FFF
.pagination_mod-1
	margin: 0 40px
	background: #FFF
	border-top: 1px solid #bfbfbf
//actual
.actual
	margin-bottom: 28px
	padding: 20px
	background: #ececec 
	border-radius: 0 0 3px 3px
	overflow: hidden
	li
